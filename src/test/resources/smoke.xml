<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="SmokeTest" verbose="1">
    <test name="PositiveTests">
        <classes>
            <class name="Homework_project.LoginTests">
                <methods>
                    <include name=".*Positive.*"/>
                </methods>
            </class>

            <class name="Homework_project.LoginTests">
                <methods>
                    <exclude name=".*Negative.*"/>
                </methods>
            </class>

        </classes>
    </test>
</suite>